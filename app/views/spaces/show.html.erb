

<div class="container">
  <div class="two_columns">
        <% if @space.images? %>

          <div class="card" style="background-image: url(<%= cl_image_path(@space.images.last.path) %>); width: 200px; height: 300px, crop: :fill ; ">
          </div>
           <% else %>
        <div class="card-header" style="background-image: url(); height: 200px;">
        </div>
        <% end %>
      <div class="card-contents" style = "top:50px; width:50vw">


        <div class="title2">
        <%= @space.name %>
        </div>
      <div class="icon-box">
        <div class="icon-row-divider">
        <p><%= @space.category %></p>
        <p>cap:<%= @space.capacity %></p>
        <p>noise:<%= @space.noise %></p>
        </div>
        <p><%= @space.description %></p>
        <div style="font-size:30px; padding:15px;">
          <%= link_to 'Inquire', new_space_event_path(@space) %>|
          <%= link_to 'Edit', edit_space_path(@space) %>|
          <%= link_to 'Back', spaces_path %>
        </div>
      </div>
  </div>
</div>

<div class="slider-container">
  <div class="slider">
    <div>1</div>
    <div>2</div>
    <div>3</div>
    <div>4</div>
    <div>5</div>
    <div>6</div>
  </div>
</div>
</div>

