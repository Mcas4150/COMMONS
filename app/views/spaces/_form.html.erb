
<%= simple_form_for(@space) do |f| %>

    <%= f.input :name %>
    <%= f.input :address, placeholder: "" %>
    <%= f.input :category %>
    <%= f.input :noise %>
    <%= f.input :capacity %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :images, as: :attachinary %>
    <%= f.button :submit, class: "main-btn"  %>

<% end %>
