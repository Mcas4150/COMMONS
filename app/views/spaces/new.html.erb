
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
      <h1>Create a New Space</h1>
      <%= render 'form' %>
      <%= link_to 'Back', spaces_path %>
    </div>
  </div>
</div>

<div class="container padded-top">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= simple_form_for(@space) do |f| %>
      <% end %>
    </div>
  </div>
</div>

<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyAljlH6KfD8t7Gd-IavyYX8RjYmgjxc2dM"></script>
<% end %>
